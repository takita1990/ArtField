  <div class="row">

    <h3>会員一覧</h3>
      <table class="table table-hover">
          <thead>
              <tr>
                  <th>会員ID</th>
                  <th>氏名</th>
                  <th>メールアドレス</th>
                  <th>ステータス</th>
              </tr>
          </thead>

          <tbody>
            <% @members.each do |member| %>
          	　<tr>
                  <td><%= member.id %></td>
                  <td><%= link_to member.name, admin_member_path(member.id) %></td>
                  <td><%= member.email %></td>
                  <td>
                    <% if member.is_withdeawal_status == false %>
                        <p>退会</p>
                    <% else %>
                        <p>有効</p>
                    <% end %>
                  </td>
              </tr>
            <% end %>
          </tbody>
      </table>
  </div>
