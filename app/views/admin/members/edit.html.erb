  <div class="row">
    <div class= "col-xs-6">
    <h3><%= @member.name %>さんの会員情報編集</h3>
    <%= form_for(@member, url: admin_member_path) do |f| %>
      <h4>氏名</h4>
        <%= f.text_field :name %>
      <h4>メールアドレス</h4>
        <%= f.text_field :email %>
      <h4>会員ステータス
        <label><%= f.radio_button :is_withdeawal_status, true,{:checked => true} %>有効</label>
        <label><%= f.radio_button :is_withdeawal_status, false %>無効</label></h4>

        <%= f.submit '変更を保存する', class: "btn btn-primary"%>
    <% end %>
  </div>
</div>

